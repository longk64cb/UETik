<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.uetik.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_albums"
        android:name="com.example.uetik.ui.albums.AlbumsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_albums" />

    <fragment
        android:id="@+id/navigation_online"
        android:name="com.example.uetik.ui.online.OnlineFragment"
        android:label="@string/title_online"
        tools:layout="@layout/fragment_online" />
    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.uetik.ui.user.UserFragment"
        android:label="@string/title_user"
        tools:layout="@layout/fragment_user"
        />

</navigation>